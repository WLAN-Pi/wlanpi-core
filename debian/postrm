#!/bin/bash
# postrm script
#
# see: dh_installdeb(1)

function unlink_if_existing() {
    if [ -L "$1" ]; then
        unlink $1
    fi
}

# unlink_if_existing sample usage:
# unlink_if_existing /etc/nginx/nginx.conf

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
